# Originally an example in demo/regression/
booster = gbtree
objective = reg:squarederror
eta = 1.0
gamma = 1.0
seed = 0
min_child_weight = 0
max_depth = 8
verbosity = 1
tree_method = hist
eval_metric = auc

num_round = 4
save_period = 0
data = "../../data/a9a.guest.train?format=libsvm"
eval[test] = "../../data/a9a.guest.test?format=libsvm"
model_dir = ../../models/
model_out = ../../models/a9a.json
dump_format = json